{
  "compilerOptions": {
      "lib": ["dom", "dom.iterable", "esnext"],
      "types": ["firebase"],
      "allowJs": true,
      "skipLibCheck": true,
      "strict": true,
      "noEmit": true,
      "esModuleInterop": true,
      "module": "esnext",
      "moduleResolution": "bundler",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx": "preserve",
      "incremental": true,
      "plugins": [
          {
              "name": "next"
          }
      ],
      "paths": {
          "@/*": ["./*"]
      }
    },
  "include": [
      "next-env.d.ts",
      "**/*.ts",
      "**/*.tsx",
      ".next/types/**/*.ts",
      "app/university/toudai.js"
, "app/mail/page.jsx", "app/toudai/page.jsx"  ],
  "exclude": ["node_modules"]
}
